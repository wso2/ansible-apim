[server]
hostname = "{{ hostname }}"
node_ip = "127.0.0.1"
offset= "{{ ports_offset }}"
server_role = "api-key-manager"

[user_store]
type = "database_unique_id"

[super_admin]
username = "{{ admin_username }}"
password = "{{ admin_password }}"
create_admin_account = true

[database.apim_db]
type = "{{ wso2am_db_type }}"
url = "{{ wso2am_db_url }}"
username = "{{ wso2am_db_username }}"
password = "{{ wso2am_db_password }}"

[database.shared_db]
type = "{{ wso2shared_db_type }}"
url = "{{ wso2shared_db_url }}"
username = "{{ wso2shared_db_username }}"
password = "{{ wso2shared_db_password }}"

[keystore.tls]
file_name =  "{{ tls_key_store_name }}"
type =  "JKS"
password =  "{{ tls_key_store_password }}"
alias =  "{{ tls_key_store_key_alias }}"
key_password =  "{{ tls_key_store_key_password }}"

[keystore.primary]
file_name =  "{{ primary_key_store_name }}"
type =  "JKS"
password =  "{{ primary_key_store_password }}"
alias =  "{{ primary_key_store_key_alias }}"
key_password =  "{{ primary_key_store_key_alias }}"

[keystore.internal]
file_name =  "{{ internal_key_store_name }}"
type =  "JKS"
password =  "{{ internal_key_store_password }}"
alias =  "{{ internal_key_store_key_alias }}"
key_password =  "{{ internal_key_store_key_alias }}"

[truststore]
file_name = "{{ trust_store_name }}"
type = "JKS"
password = "{{ trust_store_password }}"

[[event_handler]]
name="userPostSelfRegistration"
subscriptions=["POST_ADD_USER"]
